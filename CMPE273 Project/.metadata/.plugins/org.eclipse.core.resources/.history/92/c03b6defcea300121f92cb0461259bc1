      <parameter name="wsdlPortType" value="Service"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation xmlns:operNS="http://Connections" xmlns:retNS="http://Connections" xmlns:rtns="http://www.w3.org/2001/XMLSchema" name="helloWorld" qname="operNS:helloWorld" returnQName="retNS:helloWorldReturn" returnType="rtns:string" soapAction="">
      </operation>
      <parameter name="allowedMethods" value="helloWorld"/>

  </service>
