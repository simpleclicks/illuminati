<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="univesitydb" match-name=".*"/>
  <table-filter match-catalog="univesitydb" match-name="course"/>
  <table-filter match-catalog="univesitydb" match-name="courseinstructormap"/>
  <table-filter match-catalog="univesitydb" match-name="coursestudentmap"/>
  <table-filter match-catalog="univesitydb" match-name="instructor"/>
  <table-filter match-catalog="univesitydb" match-name="person"/>
  <table-filter match-catalog="univesitydb" match-name="student"/>
</hibernate-reverse-engineering>